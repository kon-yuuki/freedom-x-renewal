{
  "name": "freedom-x",
  "version": "2.2.1",
  "author": "kon",
  "license": "MIT",
  "private": true,
  "config": {
    "server": true,
    "src": "src",
    "public": "public_html",
    "css": {
      "sass": true,
      "src": "sass",
      "dest": "wp-content/themes/landi_renewal/src/renewal/css",
      "minify": true
    },
    "js": {
      "webpack": true,
      "src": "js",
      "dest": "wp-content/themes/landi_renewal/src/renewal/js",
      "entry": "main,loading"
    },
    "image": {
      "compress": true,
      "src": "imgs",
      "dest": "wp-content/themes/landi_renewal/src/renewal/images",
      "avif": true,
      "webp": true,
      "jpg": false,
      "png": false
    }
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not IE 11",
    "Android >= 11.0"
  ],
  "scripts": {
    "start": "NODE_ENV=production npm run init",
    "dev": "NODE_ENV=development npm run init",
    "init": "node ./task/init.js && npm run server & npm run css:watch & npm run js:watch & npm run image:watch",
    "server": "node --trace-warnings ./task/server.js",
    "build": "NODE_ENV=production npm run css:build & npm run js:build & npm run image:build",
    "css:watch": "if [[ $npm_package_config_css_sass ]]; then node ./task/sass.js watch; fi",
    "css:build": "if [[ $npm_package_config_css_sass ]]; then node ./task/sass.js build; fi",
    "js:watch": "if [[ $npm_package_config_js_webpack ]]; then webpack -w --config ./task/webpack.config.js; fi",
    "js:build": "if [[ $npm_package_config_js_webpack ]]; then webpack --config ./task/webpack.config.js; fi",
    "image:watch": "if [[ $npm_package_config_image_compress ]]; then node ./task/image.js watch; fi",
    "image:build": "if [[ $npm_package_config_image_compress ]]; then node ./task/image.js build; fi"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^9.1.3",
    "browser-sync": "^2.27.7",
    "chalk": "4.1.2",
    "chokidar": "^3.5.3",
    "core-js": "^3.21.1",
    "cpx": "^1.5.0",
    "del": "^6.0.0",
    "docker-compose": "^0.24.1",
    "fs-extra": "^10.0.1",
    "imagemin": "7.0.1",
    "imagemin-avif": "^0.1.4",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "9.0.0",
    "imagemin-webp": "6.0.0",
    "ip": "^1.1.8",
    "make-dir": "^3.1.0",
    "postcss": "^8.4.7",
    "postcss-cli": "^9.1.0",
    "process": "^0.11.10",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.49.9",
    "terser-webpack-plugin": "^5.3.1",
    "vue-loader": "^17.0.0",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@pmndrs/branding": "^0.0.8",
    "@splidejs/splide": "^4.1.4",
    "@splidejs/splide-extension-auto-scroll": "^0.5.3",
    "@studio-freight/lenis": "^1.0.29",
    "css-loader": "^6.9.0",
    "gsap": "^3.12.4",
    "imagesloaded": "^5.0.0",
    "jquery": "^3.6.0",
    "jquery.easing": "^1.4.1",
    "lenis": "^1.2.3",
    "lodash": "^4.17.21",
    "lottie-web": "^5.12.2",
    "perfect-scrollbar": "^1.5.5",
    "raw-loader": "^4.0.2",
    "splitting": "^1.0.6",
    "swiper": "^8.0.6",
    "tween-functions": "^1.2.0",
    "vue": "^3.2.31",
    "yakuhanjp": "^4.1.1"
  },
  "resolutions": {
    "sharp": "0.31.1"
  },
  "volta": {
    "node": "16.14.0",
    "yarn": "1.22.17"
  }
}
